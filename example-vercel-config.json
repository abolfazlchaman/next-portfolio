{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/en/(.*)",
      "dest": "/en/$1"
    },
    {
      "src": "/fa/(.*)",
      "dest": "/fa/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/en/$1"
    }
  ]
}
